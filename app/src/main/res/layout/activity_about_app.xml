<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AboutAppActivity">

    <TextView
        android:id="@+id/Andi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Andi Dong"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/Wenxin"/>

    <TextView
        android:id="@+id/Wenxin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wenxin Yang"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Andi"
        app:layout_constraintBottom_toTopOf="@id/Xiaoqing"/>

    <TextView
        android:id="@+id/Xiaoqing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Xiaoqing Meng"
        app:layout_constraintBottom_toTopOf="@id/Zijian"
        app:layout_constraintTop_toBottomOf="@id/Wenxin"/>

    <TextView
        android:id="@+id/Zijian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Zijian Huang"
        app:layout_constraintBottom_toTopOf="@id/TeamName"
        app:layout_constraintTop_toBottomOf="@id/Xiaoqing"/>

    <TextView
        android:id="@+id/TeamName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="TEAM 48"
        app:layout_constraintTop_toBottomOf="@id/Zijian"/>

</androidx.constraintlayout.widget.ConstraintLayout>